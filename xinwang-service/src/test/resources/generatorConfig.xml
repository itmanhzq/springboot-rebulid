<!DOCTYPE generatorConfiguration
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!--suppress MybatisGenerateCustomPluginInspection -->
<generatorConfiguration>
    <context id="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">

        <property name="beginningDelimiter" value="`"/>
        <property name="endingDelimiter" value="`"/>

        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="com.fenlibao.xinwang.common.BaseMapper"/>
            <property name="caseSensitive" value="true"/>
        </plugin>

       <plugin type="com.fenlibao.xinwang.config.LombokConfig" >
            <property name="hasLombok" value="true"/>
        </plugin>

        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://192.168.40.239:3306/xinwang?characterEncoding=utf8"
                        userId="root" password="fenlibao.123">
        </jdbcConnection>

        <!--MyBatis 生成器只需要生成 Model-->
        <javaModelGenerator targetPackage="com.fenlibao.xinwang.model.po"
                            targetProject="src/main/java"/>

        <sqlMapGenerator targetPackage="com.fenlibao.xinwang.test"
                         targetProject="src/test/java/resources/test"/>

        <javaClientGenerator targetPackage="com.fenlibao.xinwang.mapper"
                             targetProject="src/main/java"
                             type="XMLMAPPER"/>


        <!--<table tableName="interface_privilege"/>-->
        <!--<table tableName="request_log"/>-->
    </context>
</generatorConfiguration>